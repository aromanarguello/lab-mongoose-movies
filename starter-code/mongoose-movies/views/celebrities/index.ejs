<h2> Celebrities </h2>

  <% listOfCelebrities.forEach( oneCeleb => { %>
    <ul>
      <li>
        <%= oneCeleb.name %>
        <a href="/celebrities/<%= oneCeleb._id %>">
          <p>Details</p>
        </a>
      </li>
    </ul>
    <br />

    <form action="/celebrities/<%=oneCeleb._id%>/delete" method="post">
    <button> Delete</button>
    </form>
  <% }); %>

  <a href="/celebrities/new">
    <p>Add</p>
  </a>
